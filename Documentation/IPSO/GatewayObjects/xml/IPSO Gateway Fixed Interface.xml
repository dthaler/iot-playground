<?xml version="1.0" encoding="utf-8"?>
<LWM2M>
    <Object ObjectType="MODefinition">
        <Name>IPSO Gateway Fixed Interface</Name>
        <Description1>This IPSO Object is used to represent some settings for the fixed interface.</Description1>
        <ObjectID>X</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:X</ObjectURN>
        <MultipleInstances>Multiple</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="0">
                <Name>Name</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource type returns the name of the fixed interface.</Description>
            </Item>            
            <Item ID="1">
                <Name>Mac Address</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource type returns the MAC address of the fixed interface.</Description>
            </Item>      
            <Item ID="2">
                <Name>Interface Status</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Boolean</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource type returns status of the interface (1: Up,  0: Down)</Description>
            </Item>  
            <Item ID="3">
                <Name>IP Addresses</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Multiple</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource type returns an array of IP addresses.</Description>
            </Item>  
        </Resources>
        <Description2></Description2>
    </Object>
</LWM2M>